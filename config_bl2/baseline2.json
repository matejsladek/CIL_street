{
      "name": "baseline2",
      "dataset": "maps1800_all",
      "img_resize": 384,
      "v_flip": 0,
      "h_flip": 0,
      "rot": 0,
      "contrast": 0,
      "brightness": 0,
      "normalize": true,
      "backbone": "seresnext101",
      "predict_distance": false,
      "predict_contour": false,
      "batch_size": 1,
      "epochs": 25,
      "optimizer": "adam",
      "learning_rate": 0.0001,
      "loss": [
            "binary_crossentropy",
            "binary_crossentropy",
            "mse"
      ],
      "loss_weights": [
            1,
            0.2,
            1
      ],
      "pretrained": true,
      "postprocess": "morphological",
      "seed": 42,
      "img_size": 400,
      "img_size_test": 608,
      "n_channels": 3,
      "n_classes": 2,
      "buffer_size": 1000,
      "n_ensemble": 5,
      "hard_mask": true,
      "stop_on_metric": true,
      "aspp": false,
      "augment_loss": false,
      "custom_callback": true,
      "bagging_threshold": 90,
      "residual": false,
      "art": true,
      "se": true,
      "experimental_decoder": true,
      "decoder_exp_setting": "A",
      "use_cv": true,
      "cv_k": 5,
      "cv_k_todo": 5,
      "save_model": false,
      "use_baseline_code1": false,
      "use_baseline_code2": true,
      "use_ensemble": false
}
