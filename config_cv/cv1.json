{
  "name": "experiment_cv_dev",
  "dataset": "original_all",
  "img_resize": 384,
  "v_flip": 0.5,
  "h_flip": 0.5,
  "rot": 0.25,
  "contrast": 0.2,
  "brightness": 0.2,
  "normalize": true,
  "backbone": "seresnext101",
  "predict_distance": false,
  "predict_contour": true,
  "batch_size": 4,
  "epochs": 30,
  "optimizer": "adam",
  "learning_rate": 0.0001,
  "loss": ["binary_crossentropy", "binary_crossentropy", "mse"],
  "loss_weights": [1, 1, 1],
  "pretrained": true,
  "postprocess": "morphological",
  "seed": 42,
  "use_cv": true,
  "cv_k": 5,
  "cv_k_todo": 5,
  "img_size": 400,
  "img_size_test": 608,
  "n_channels": 3,
  "n_classes": 2,
  "buffer_size": 1000,
  "n_ensemble": 3,
  "hard_mask": true,
  "stop_on_metric": false,
  "aspp": false
}
